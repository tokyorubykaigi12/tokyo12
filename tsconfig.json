{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ES6",
    "module": "es2015",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsxImportSource": "preact"
  },
  "include": ["src"]
}
